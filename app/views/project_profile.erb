
<div class="container" align="center" id="blue">
  <h1><%=@project.name %></h1>
</div><br>

<div class="jumbotron">
  <div class="container" align="center">
    <%= youtube_embed(@youtube)%>
  </div>
</div>



<div class="container-fluid">
  <div class="jumbotron text-center" id="jumbo">
    <!-- <h1><%=@project.name %></h1> -->
    <h3>Date <br> <%=@project.start_date%> - <%=@project.end_date%> <br><br>
    Location <br> <%=@project.location%> <br><br>
    Description <br> <%=@project.bio%> <br><br>
    Volunteers Needed <br> <%=@project.number_of_vols%></h3>
  </div>
</div>

<div class="container" align="center">
  <% if current_user %>
  <form action="/projects/<%= @project.id %>" method="post">
      <input type="submit" class="btn btn-primary" value="Join this Project!">
  </form>
  <%end%>
</div>

<br>
